         point="org.eclipse.wst.server.core.moduleFactories">
      <moduleFactory
          
            projects="true"
            
            class="org.eclipse.jst.j2ee.internal.deployables.ApplicationClientDeployableFactory"
            id="com.ibm.wtp.server.j2ee.appclient">
